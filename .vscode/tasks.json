{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build (debug)",
			"type": "swift",
			"args": [
				"package",
				"--triple",
				"armv6m-none-none-eabi",
				"--toolchain",
				"/Library/Developer/Toolchains/swift-latest.xctoolchain/",
				"link",
				"--configuration", "debug",
				"--objcopy", "/opt/homebrew/opt/llvm/bin/llvm-objcopy"
			],
			"disableTaskQueue": true,
			"problemMatcher": [
				"$swiftc"
			],
			"group": "build",
		},
		{
			"label": "Build (release)",
			"type": "swift",
			"args": [
				"package",
				"--triple",
				"armv6m-none-none-eabi",
				"--toolchain",
				"/Library/Developer/Toolchains/swift-latest.xctoolchain/",
				"link",
				"--configuration", "release",
				"--objcopy", "/opt/homebrew/opt/llvm/bin/llvm-objcopy"
			],
			"disableTaskQueue": true,
			"problemMatcher": [
				"$swiftc"
			],
			"group": "build",
		}
	]
}